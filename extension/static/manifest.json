{
  "manifest_version": 2,
  "name": "Hentai Downloader",
  "version": "0.1",

  "description": "An addon that downloads the content of the open tabs",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/icon-48.png"
  },

  "permissions": ["tabs", "notifications"]
}
